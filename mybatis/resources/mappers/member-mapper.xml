<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="memberMapper">
  <select id="checkId" resultType="_int">
    select count(*) from Member where user_id = #{userId}
  </select>
  <select id="insertMember" parameterType="com.mybatis.member.dto.Member">
    insert into member values(#{userId},#{userPwd},#{userName},#{email},#{birthday},#{gender},#{phone},#{address},default,default,default)
  </select>


</mapper>













